        <!-- End Interactive Slider v2 -->
        <!--=== Content Part ===-->
        @RenderBody()
        <!-- End Content Part -->
        <!--=== Footer v8 ===-->
        @await Html.PartialAsync("_Footer")
        <!--=== End Footer v8 ===-->
