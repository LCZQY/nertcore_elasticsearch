    <link rel="stylesheet" href="//static.dotnet.live/assets/css/theme-skins/dark.css" asp-append-version="true" />

    <!-- CSS Customization -->
    <link rel="stylesheet" href="//static.dotnet.live/assets/css/custom.css" asp-append-version="true" /> @Html.Raw(JavaScriptSnippet.FullScript)
</head>

<body class="header-fixed header-fixed-space-v2">
    <div class="wrapper">
        <!--=== Header v8 ===-->
        @await Html.PartialAsync("_Header")
        <!--=== End Header v8 ===-->
        <!-- Interactive Slider v2 -->
